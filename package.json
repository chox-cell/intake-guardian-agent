    "smoke": "bash scripts/smoke.sh",
    "lint:types": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "express": "^4.19.2",
    "zod": "^3.24.1",
    "sqlite3": "^5.1.7",
    "pino": "^9.5.0",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "@types/express": "^4.17.21"
  }
}
