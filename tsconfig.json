{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "__bak_*",
    "__bak_salespack_*",
    "**/*.bak.*",
    ".bak",
    "__bak_resend_*",
    "__bak_fix_*",
    "__bak_ui_v*",
    "__bak_ui_*"
  ]
}
